<body class="hold-transition skin-purple sidebar-mini">

<div class="wrapper">

    <!-- 页面头部 -->
   <jsp:include page="../pages/header.jsp"/>
    <!-- 页面头部 /-->

    <!-- 导航侧栏 -->
    <jsp:include page="./asidebar.jsp"/>
    <!-- 导航侧栏 /-->

    <!-- 内容区域 -->
    <!-- @@master = admin-layout.html-->
    <!-- @@block = content -->

    <div class="content-wrapper">

        <!-- 内容头部 -->
        <section class="content-header">
            <h1>
                系统日志
                <small>系统日志</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
                <li><a href="/sysLog/findAll.do">系统日志</a></li>
            </ol>
        </section>
        <!-- 内容头部 /-->

        <!-- 正文区域 -->
        <section class="content">

            <!-- .box-body -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">列表</h3>
                </div>

                <div class="box-body" >

                    <!-- 数据表格 -->
                    <div class="table-box ">

                        <!--工具栏-->
                        <div class="pull-left">
                            <div class="form-group form-inline">
                                <div class="btn-group">
                                    <button type="button" class="btn btn-default" title="刷新" id="btn_reflush"><i class="fa fa-refresh"></i> 刷新</button>
                                </div>
                            </div>
                        </div>
                        <div class="box-tools pull-right">
                            <div class="has-feedback" id="search_content">
                                <input type="text" class="form-control input-sm" placeholder="搜索" id="input_content">
                                <span class="glyphicon glyphicon-search form-control-feedback"></span>
                            </div>
                        </div>
                        <!--工具栏/-->

                        <!--数据列表-->
                        <table id="dataList" class="table table-bordered table-striped table-hover dataTable  " style="font-size: smaller">
                            <thead>
                            <tr>
                                <th class="sorting">ID</th>
                                <th class="sorting">visitTime</th>
                                <th class="sorting">用户</th>
                                <th class="sorting">IP</th>
                                <th class="sorting">执行时间</th>
                                <th class="sorting">URL</th>
                                <th class="sorting">方法</th>
                            </tr>
                            </thead>
                            <tbody>

                            <c:forEach items="${sysLogs.list}" var="sysLog" varStatus="s">
                                <tr>
                                    <td>${sysLog.id}</td>
                                    <td>${sysLog.visitTimeStr}</td>
                                    <td>${sysLog.username}</td>
                                    <td> ${sysLog.ip}</td>
                                    <td> ${sysLog.executionTime}</td>
                                    <td> ${sysLog.url}</td>
                                    <td> ${sysLog.method}</td>
                                </tr>
                            </c:forEach>




                            </tbody>

                        </table>
                        <!--数据列表/-->


                    </div>
                    <!-- 数据表格 /-->


                </div>
                <!-- /.box-body -->

                <!-- .box-footer-->
                <div class="box-footer">
                    <div class="pull-left">
                        <div class="form-group form-inline">
                            总共${sysLogs.totalPages} 页，共${sysLogs.totalCount}条数据。 每页
                            <select class="form-control" id="page_selector">
                                <option value="10">10</option>
                                <option value="15">15</option>
                                <option value="20">20</option>
                                <option value="25">25</option>
                                <option value="30">30</option>
                            </select> 条
                        </div>
                    </div>

                    <div class="box-tools pull-right">
                        <ul class="pagination">
                            <li>
                                <a href="/sysLog/findAll.do?pageNum=1&pageSize=${sysLogs.pageSize}" aria-label="Previous">首页</a>
                            </li>
                            <c:forEach begin="1" end="${sysLogs.totalPages}" varStatus="s">
                                <li><a href="/sysLog/findAll.do?pageNum=${s.index}&pageSize=${sysLogs.pageSize}">${s.index}</a></li>
                            </c:forEach>
                            <li>
                                <a href="/sysLog/findAll.do?pageNum=${sysLogs.totalPages}&pageSize=${sysLogs.pageSize}" aria-label="Next">尾页</a>
                            </li>
                        </ul>
                    </div>

                </div>
                <!-- /.box-footer-->



            </div>

        </section>
        <!-- 正文区域 /-->

    </div>
    <!-- @@close -->
    <!-- 内容区域 /-->

    <!-- 底部导航 -->
    <jsp:include page="./footer.jsp"/>
    <!-- 底部导航 /-->

</div>



</body>
</html>
